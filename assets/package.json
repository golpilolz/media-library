{
  "name": "@golpilolz/medialibrary",
  "description": "Media library for Symfony Forms",
  "version": "1.0.0",
  "main": "dist/controller.js",
  "types": "dist/controller.d.ts",
  "config": {
    "css_source": "src/style.css"
  },
  "symfony": {
    "controllers": {
      "media-library": {
        "main": "dist/controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "@golpilolz/media-library/dist/style.min.css": true
        }
      }
    },
    "importmap": {
      "@hotwired/stimulus": "^3.0.0"
    }
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@hotwired/stimulus": "^3.0.0",
    "typescript": "^4.9.5",
    "csso-cli": "^4.0.2"
  },
  "scripts": {
    "build": "babel src --extensions .ts -d dist",
    "build:css": "csso src/style.css -o dist/style.min.css"
  }
}
